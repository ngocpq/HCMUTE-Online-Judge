@model SPKTOnline.Models.Student_Submit
@{
    ViewBag.Title = "TryTestResult";
    Layout = "~/Views/Shared/_Layout.cshtml";
    int k = 0;
}
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
<div class="webtitle">
    Môn: @Model.Problem.Subject.Name <br />
    Người làm bài: @Model.User.LastName @Model.User.FirstName <span>&nbsp-&nbsp</span> @Model.User.Username<br />
    Kết quả bài: @Model.Problem.Name </div>
<p>@Html.Raw(@ViewBag.Message)</p>


  <p>Kết quả biên dịch: @Model.KetQuaBienDich
    @if (@Model.TrangThaiBienDich!=1)
    {
        <b>Lỗi biên dịch:</b><i>@Model.CompilerError</i>
    }
  </p>
  <p>Tổng điểm    <b>@Model.TongDiem</b></p>
  Kết quả chi tiết
  @if (@Model.TrangThaiBienDich == 1)
  {
        <table style="width: 650px">
        <tr>
            <th>
                Testcase
            </th>
            <th>
                Mô tả testcase
            </th>
            <th>
                Điểm
            </th>
            <th>
                Run Time
            </th>
            <th>
                Ghi chú
            </th>
         </tr> 
            @{int j = 0;}
            @foreach (var i in @Model.TestCaseResults)
            {
                
                <tr> 
                    
                <td>
                    @*@i.TestCaseID*@
                    @{j = j + 1;}
                    @j.ToString()
                    
                </td>                                   
                 <td>
                    @i.TestCas.MoTa
                </td>                    
                <td>
                    @i.Score
                </td>
                <td>
                    @i.ExecutionTime
                </td>
                <td>
                    @i.Comment
                </td>
                </tr>
                             
            }
  
        </table>
  }
<div class="lambai" style="color:Olive;border-bottom-color:Gray; border-bottom-style:ridge">
    Xem source code |</div>

  
    

